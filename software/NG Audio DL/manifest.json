{
	"name": "Newgrounds Audio Downloader",
	"short_name": "NG Audio DL",
	"description": "Can download any audio file submitted to the audio portal, even if the download button is disabled",
	"version": "0.75",
	"manifest_version": 3,
	"background": {"service_worker": "main.js"},
	"content_scripts": [{"matches": ["https://www.newgrounds.com/audio/listen/?*"]}],
	"permissions": ["storage", "activeTab", "scripting"],
	"host_permissions": ["https://audio.ngfiles.com/"],
	"icons": {"any": "icon.svg"}
}
